{
    "name": "samplesite",
    "author": "stratos",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite build && vite ./build --port 4173",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "format": "prettier --write .",
        "lint": "prettier --check ."
    },
    "devDependencies": {
        "@sveltejs/adapter-static": "^3.0.10",
        "@sveltejs/kit": "^2.50.0",
        "@sveltejs/vite-plugin-svelte": "^6.2.4",
        "@tailwindcss/vite": "^4.1.18",
        "prettier": "^3.8.0",
        "prettier-plugin-svelte": "^3.4.1",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "svelte": "^5.47.1",
        "svelte-check": "^4.3.5",
        "tailwindcss": "^4.1.18",
        "typescript": "^5.9.3",
        "vite": "^7.3.1"
    },
    "prettier": {
        "semi": false,
        "tabWidth": 4,
        "singleQuote": true,
        "trailingComma": "all",
        "svelteSortOrder": "scripts-options-markup-styles",
        "svelteStrictMode": false,
        "svelteBracketNewLine": true,
        "svelteAllowShorthand": true,
        "plugins": [
            "prettier-plugin-svelte",
            "prettier-plugin-tailwindcss"
        ]
    },
    "engines": {
        "node": ">= 22"
    },
    "type": "module",
    "pnpm": {
        "ignoredBuiltDependencies": [
            "esbuild"
        ],
        "onlyBuiltDependencies": [
            "@tailwindcss/oxide",
            "esbuild"
        ],
        "overrides": {
            "cookie@<0.7.0": ">=0.7.0"
        }
    },
    "dependencies": {
        "@types/node": "^24.10.9"
    }
}
